/* 
Do not edit this files manualy, it is gereated automaticly by gulp.
Instead edit gulp/templates/sprites.js.
*/

{{#shapes}}
    {{#first}}
    .icon {
        background-image: url('../../assets/images/sprites/{{{sprite}}}');
    }
    {{/first}}

    .icon--{{base}} {
       width: {{width.outer}}px;
       height: {{height.outer}}px; 
       background-position: {{position.relative.xy}};
    }
{{/shapes}}